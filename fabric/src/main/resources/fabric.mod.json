  "authors": [
    "Starfish Studios"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/another-furniture",
    "issues": "https://github.com/starfish-studios/AnotherFurniture/issues"
  },
  "license": "MIT",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.starfish_studios.another_furniture.fabric.AnotherFurnitureFabric"
    ],
    "client": [
      "com.starfish_studios.another_furniture.fabric.AnotherFurnitureFabricClient"
    ]
  },
  "mixins": [
    "another_furniture.mixins.json",
    "another_furniture-common.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.6",
    "minecraft": ">=1.18.2"
  }
}
